{
  "name": "corewar",
  "version": "0.0.50",
  "description": "HTML5 & javascript implementation of the classic game [corewar](https://en.wikipedia.org/wiki/Core_War)",
  "main": "./dist/index.js",
  "scripts": {
    "test": "nyc mocha",
    "start": "webpack-dev-server --progress --colors",
    "prepublishOnly": "webpack"
  },
  "files": [
    "**/dist/index.js",
    "**/dist/"
  ],
  "types": [
    "**/dist/index.d.ts"
  ],
  "nyc": {
    "extension": [
      ".ts"
    ],
    "exclude": [
      "**/*[Tt]ests.ts",
      "**/Test*.ts",
      "**/*Helper.ts",
      "dist"
    ],
    "reporter": [
      "text",
      "lcov"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/gareththegeek/corewar.git"
  },
  "author": "Gareth Williams",
  "contributors": [
    {
      "name": "Doug McDonald"
    }
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/gareththegeek/corewar/issues"
  },
  "homepage": "https://github.com/gareththegeek/corewar#readme",
  "devDependencies": {
    "@types/node": "^8.0.51",
    "@types/mocha": "^2.2.44",
    "chai": "^4.1.2",
    "mocha": "^4.0.1",
    "nyc": "^11.3.0",
    "sinon": "^4.1.2",
    "sinon-chai": "^2.14.0",
    "ts-loader": "^3.1.1",
    "ts-mocha": "^1.0.3",
    "ts-node": "^3.3.0",
    "typescript": "^2.6.1",
    "webpack": "^3.8.1"
  },
  "dependencies": {
    "@types/core-js": "^0.9.43",
    "core-js": "^2.5.1",
    "pubsub-js": "^1.5.7",
    "underscore": "^1.8.3"
  }
}
